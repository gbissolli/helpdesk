@model HelpDesk.Mvc.Models.Pessoas

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
</head>
<body>
    <div>
        <form action="/Home/Login" method="post">
            Email: <input type="text" id="login" name="EMAIL" required/>
